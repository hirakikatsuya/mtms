<%= form_with model:group do |f| %>
  <div class="form-group">
    <%= f.label "グループ名" %>
    <%= f.text_field:group_name,class:"form-control" %>
  </div>
  <div class="form-group">
    <%= f.label "グループ画像" %>
    <%= f.file_field:group_image %>
  </div>
  <div class="form-group">
    <%= f.label "グループ詳細" %><br>
    <%= f.text_area:group_explain,class:"form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit "登録" ,class:"btn btn-success" %>
  </div>
<% end %>